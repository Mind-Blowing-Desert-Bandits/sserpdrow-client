

 <!-- Developers, why are we using data-id and not just id? -->
 <div>
   <ul data-id="{{page.id}}">
     <h1>
     {{#if page.title}}
     <small><b>Title:</b> {{page.title}}</small>
     {{else}}
     <b>No Title</b>
     {{/if}}</h1>

     <p>
     {{#if page.textcontent}}
     <small> {{page.textcontent}}</small>
     {{else}}
     <b>No Content</b>
     {{/if}}</p>

     <br>
   </ul>

 </div>
